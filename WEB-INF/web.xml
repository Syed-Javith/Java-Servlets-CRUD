<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

    <servlet>
      <servlet-name>GetEmployees</servlet-name>
      <servlet-class>GetEmployees</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>GetEmployees</servlet-name>
        <url-pattern>/GetEmployees</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>InsertEmployee</servlet-name>
      <servlet-class>InsertEmployee</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>InsertEmployee</servlet-name>
        <url-pattern>/InsertEmployee</url-pattern>
    </servlet-mapping>

     <servlet>
      <servlet-name>DeleteEmployee</servlet-name>
      <servlet-class>DeleteEmployee</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>DeleteEmployee</servlet-name>
        <url-pattern>/DeleteEmployee</url-pattern>
    </servlet-mapping>

</web-app>
